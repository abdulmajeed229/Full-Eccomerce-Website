{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/profile/page.jsx", "app/sign-up/page.tsx", "app/[id]/page.jsx", "app/cart/page.tsx", "app/AdminOrdersPage/page.jsx", "app/admin/page.jsx", "app/componets/AddProductForm.tsx", "app/componets/users.tsx", "app/componets/orders.jsx", "app/admin-login/page.tsx"],
  "exclude": ["node_modules"]
}
